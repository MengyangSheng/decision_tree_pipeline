{
  "type": "score_model",
  "feature_order": ["wind_speed_mps", "dry_bulb_temperature", "precipitation_mm", "relative_humidity_pct"],
  "bins": {
    "wind_speed_mps": [[0, 3], [3, 8], [8, "inf"]],
    "dry_bulb_temperature": [[-50, 5], [5, 25], [25, "inf"]],
    "precipitation_mm": [[5, "inf"], [0.1, 5], [0, 0.1]],
    "relative_humidity_pct": [[60, "inf"], [30, 60], [0, 30]]
  },
  "rule": "lambda i1, i2, i3, i4, s: 8*i1 + 4*i2 + 2*i3 + 1*i4"
}